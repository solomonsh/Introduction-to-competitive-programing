    
    static void trianlge2(){
        for(int i = 1; i<=5;i++){
            for(int j=5;j-i>0;j--){
                System.out.print(" ");
            }
            for(int k = 0;k<2*i-1;k++){
                System.out.print("*");

            }
              System.out.println();
        }
     
        }
